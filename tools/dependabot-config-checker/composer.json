{
    "name": "ramona/automation-platform-tool-dependabot-config-checker",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Ramona\\AutomationPlatformToolDependabotConfigChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Ramona\\AutomationPlatformToolDependabotConfigChecker\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../libraries/php/*/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "symfony/yaml": "^5.3",
        "thecodingmachine/safe": "^1.3"
    },
    "require-dev": {
        "ramona/automation-platform-lib-coding-standard": "*",
        "symplify/easy-coding-standard": "^9.4",
        "vimeo/psalm": "^4.13",
        "phpunit/phpunit": "^9.5",
        "infection/infection": "^0.25.3",
        "slevomat/coding-standard": "^7.0"
    }
}
